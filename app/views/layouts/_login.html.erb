<% unless current_user %>
<div class="col-md-4 col-md-offset-4">
<div class="panel panel-primary" style="margin-top:10px;">
  <div class="panel-heading" style="text-align:center">
    <h3> Log in </h3>
  </div>
  <div class="panel-body">
    <%= form_tag sessions_path do %>
    <div class="col-md-2 col-md-offset-3" style="text-align:right;"> <!--  margin:auto; max-width:39%"> -->
      <label> Username </label>
      <%= text_field_tag :netid, '', autofocus: "autofocus"%><br />
      <%= label_tag :password %>
      <%= password_field_tag :password %>
      <button class="btn btn-primary" style="margin-top:8px" type="submit">Sign in</button>
     </div>
    <% end %>
  </div>
</div>
</div>
<% end %>
