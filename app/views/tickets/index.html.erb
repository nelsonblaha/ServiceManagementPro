<div class="col-md-6 col-md-offset-3" style="padding-top:15px">
<div class="panel panel-success">

<div class="panel-heading">
<h1>Listing tickets</h1>
</div>

<div class="panel-body">
<table class="table">
  <thead>
    <tr>
      <th>Ticket</th>
      <th>Created By</th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= link_to "#{ticket.id} -- #{ticket.description}", edit_ticket_path(ticket) %></td>
        <td><%= ticket.user.netid if ticket.user%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<!-- <div class="col-md-12"> -->
</div>
<div class="panel-footer" style="text-align:center">
<div class="col-md-12">
<%= will_paginate @tickets, renderer: BootstrapPagination::Rails %>
</div>
<%= link_to 'New Ticket', new_ticket_path, class: "btn btn-primary", style: "color:white" %>
</div>
</div>
</div>

